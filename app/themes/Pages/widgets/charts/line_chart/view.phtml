<div id="w_{{widget.id}}" style="width:100%"></div><script>
$('#w_{{widget.id}}').highcharts({
            chart: {
            type: '{{parm['chart_type']}}',
            zoomType: 'xy'
        },
 title: {
            text: '{{parm['chart_title']}}',
            x: -20 //center
        },
        xAxis: {
            title: {
                text: '{{parm['x_label']}}'
            }
        },
        yAxis: {
            title: {
                text: '{{parm['y_label']}}'
            }
        },
 series: [
{% for key,series in parm['db'] %}
{
     name: '{{key|escape_js}}',
     data: [
        {% for row in series %}
    ['{{ row['x_axis']|escape_js }}',{{ row['value']|escape_js }}] ,
        {% endfor  %}
]},

{% endfor  %}
]

});
</script>{{ content() }}