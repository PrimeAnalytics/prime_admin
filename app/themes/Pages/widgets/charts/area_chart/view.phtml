<div id="w_{{widget.id}}"></div><script>
$('#w_{{widget.id}}').highcharts({
title: {
    text: '{{parm['title']}}'
},
    series: [
{% for key,series in parm['db'] %}
{
     name: '{{key|escape_js}}',
     data: [
        {% for row in series %}
    ['{{ row['x_axis']|escape_js }}',{{ row['value']|escape_js }}] ,
        {% endfor  %}
]},

{% endfor  %}
]

});
</script>{{ content() }}